{
    "name": "qf/laravel-acm-client",
    "description": "laravel aliyun acm client",
    "keywords": [
        "laravel",
        "acm client",
        "aliyun acm"
    ],
    "homepage": "https://github.com/2512422541/laravel-acm-client",
    "license": "MIT",
    "authors": [
        {
            "name": "peng",
            "homepage": "https://github.com/2512422541",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": ">=6.3"
    },
    "require-dev": {
        "orchestra/testbench": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Donjan\\AcmClient\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Donjan\\AcmClient\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Donjan\\AcmClient\\Providers\\AcmServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
